{
    "name": "TSBoilerplate",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "clear-android": "rm -rf android/build android/app/build",
        "clear-ios": "rm -rf ios/build",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "fix": "eslint --fix app/**/*.{js,jsx,ts,tsx}",
        "format": "prettier --write app/**/*.{js,jsx,ts,tsx}",
        "prettier-format": "prettier --config .prettierrc app/**/*.{js,jsx,ts,tsx} --write",
        "prettier-watch": "onchange 'app/**/*.ts' -- prettier --write {{changed}}"
    },
    "dependencies": {
        "@react-native-community/masked-view": "^0.1.10",
        "@react-navigation/native": "^5.7.1",
        "@react-navigation/stack": "^5.7.1",
        "axios": "^0.19.2",
        "react": "16.13.1",
        "react-native": "0.63.0",
        "react-native-elements": "^2.0.4",
        "react-native-gesture-handler": "^1.6.1",
        "react-native-reanimated": "^1.9.0",
        "react-native-safe-area-context": "^3.1.1",
        "react-native-screens": "^2.9.0",
        "react-native-vector-icons": "^7.0.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "reselect": "^4.0.0",
        "typesafe-actions": "^5.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/runtime": "^7.8.4",
        "@react-native-community/eslint-config": "^1.1.0",
        "@types/jest": "^25.2.3",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-native": "^0.62.0",
        "@types/react-redux": "^7.1.9",
        "@types/react-test-renderer": "^16.9.2",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-jest": "^25.1.0",
        "eslint": "^6.5.1",
        "eslint-config-airbnb-typescript-prettier": "^3.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.59.0",
        "onchange": "^7.0.2",
        "prettier": "^2.0.4",
        "react-test-renderer": "16.13.1",
        "reactotron-react-native": "^5.0.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
